<?xml version='1.0' encoding='utf-8'?>
<Defs>
	
	  <RecipeDef Abstract="True" Name="SurgeryAndroidEnergySystemImplantInstall">
    <effectWorking>Repair</effectWorking>
    <soundWorking>Recipe_Machining</soundWorking>
    <workSpeedStat>ButcheryMechanoidSpeed</workSpeedStat>
    <workSkill>Crafting</workSkill>
    <workSkillLearnFactor>16</workSkillLearnFactor>
	<anesthetize>false</anesthetize>
	<workerClass>MOARANDROIDS.Recipe_InstallEnergySystemImplant</workerClass>
	<dontShowIfAnyIngredientMissing>false</dontShowIfAnyIngredientMissing>
	<modExtensions>
		<li Class="MOARANDROIDS.DefModExt_AutoIncludeWithEnergySystems"/>
	</modExtensions>
  </RecipeDef>

	<!--        Remove implants    -->
	<RecipeDef ParentName="SurgeryAndroidEnergySystemImplantInstall">
		<defName>AT_RemoveAndroidImplant</defName>
		<label>remove android implant</label>
		<description>Removes various installed android implants</description>
		<jobString>Removing android implant</jobString>
		<skillRequirements>
			<Medicine>5</Medicine>
		</skillRequirements>
		<workerClass>MOARANDROIDS.Recipe_RemoveEnergySystemImplant</workerClass>
	</RecipeDef>
	
	<!--Pre-existing recipes-->
	
	<!--Crude-->
  <RecipeDef ParentName="SurgeryAndroidEnergySystemImplantInstall">
		<defName>InstallSmallBatteryPack</defName>
		<label>install crude battery pack</label>
		<description>Installs a crude battery pack.</description>
		
		<jobString>Installing crude battery pack.</jobString>
      <skillRequirements>
		<Crafting>3</Crafting>
      </skillRequirements>

		<workAmount>1000</workAmount>
		<surgerySuccessChanceFactor>1.30</surgerySuccessChanceFactor>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>RepairKitAT</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>SmallBatteryPack</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
					<thingDefs>
						<li>RepairKitAT</li>
				<li>SmallBatteryPack</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>MBattery</li>
		</appliedOnFixedBodyParts>
		<addsHediff>BBattery</addsHediff>
	</RecipeDef>

