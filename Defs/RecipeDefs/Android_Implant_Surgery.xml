<?xml version='1.0' encoding='utf-8'?>
<Defs>
	
	<RecipeDef Abstract="True" Name="SurgeryAndroidImplantInstall">
		<effectWorking>Surgery</effectWorking>
		<soundWorking>Recipe_Surgery</soundWorking>
		<workSpeedStat>MedicalOperationSpeed</workSpeedStat>
		<workSkill>Medicine</workSkill>
		<workSkillLearnFactor>16</workSkillLearnFactor>
		<workAmount>2500</workAmount>
		<workerClass>MOARANDROIDS.Recipe_InstallAndroidImplant</workerClass>
		<dontShowIfAnyIngredientMissing>false</dontShowIfAnyIngredientMissing>
		<modExtensions>
			<li Class="MOARANDROIDS.DefModExt_AutoIncludeWithEnergySystems"/>
		</modExtensions>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Medicine</li>
					</categories>
				</filter>
				<count>2</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Medicine</li>
			</categories>
		</fixedIngredientFilter>

	</RecipeDef>

	<!--		Install Implants 		-->
	<RecipeDef ParentName="SurgeryAndroidImplantInstall">
		<defName>AT_InstallBatteryTiny</defName>
		<label>install tiny battery</label>
		<description>Installs tiny battery</description>
		<jobString>Installing tiny battery.</jobString>
		<skillRequirements>
			<Medicine>1</Medicine>
		</skillRequirements>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>AT_EnergySystemBatteryTiny</li>
					</thingDefs>
				</filter>
				<count>1</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>AT_EnergySystemBatteryTiny</li>
			</thingDefs>
		</fixedIngredientFilter>
		<appliedOnFixedBodyParts>
			<li>MBattery</li>
		</appliedOnFixedBodyParts>
		<addsHediff>AT_ImplantedBatteryTiny</addsHediff>
	</RecipeDef>

