<?xml version="1.0" encoding="utf-8" ?>
<Defs>
  <!-- Added body parts -->

  <HediffDef Name="AddedBodyPartBaseAndroid" Abstract="True">
    <hediffClass>Hediff_AddedPart</hediffClass>
    <defaultLabelColor>(0.6, 0.6, 0.7)</defaultLabelColor>
    <isBad>false</isBad>
  </HediffDef>
 
   <HediffDef Name="BadAddedBodyPartBaseAndroid" Abstract="True">
    <hediffClass>Hediff_AddedPart</hediffClass>
    <defaultLabelColor>(0.5, 0.5, 0.5)</defaultLabelColor>
    <isBad>false</isBad>
  </HediffDef>
 
   <HediffDef>
    <defName>ShutdownAndroid</defName>
    <label>decommissioned</label>
    <initialSeverity>0</initialSeverity>
	<lethalSeverity>0</lethalSeverity>
		  		<defaultLabelColor>(0.4, 0.4, 0.4)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
  </HediffDef>
  
  
  <HediffDef>
    <defName>FreshReprogramming</defName>
    <label>recently reprogrammed</label>
	    <hediffClass>HediffWithComps</hediffClass>
    <initialSeverity>1</initialSeverity>
		  		<defaultLabelColor>(0.5, 0.5, 0.3)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
	    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-0.05</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <setMax>0.50</setMax>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <setMax>0.50</setMax>
          </li>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0.65</setMax>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
	 
  <HediffDef ParentName="ImplantHediffBase">
    <defName>OverclockedAndroid</defName>
    <label>overclocked</label>
    <labelNoun>an overclocking upgrade</labelNoun>
				<stages>
			<li>
				 <capMods>
					<li>
					  <capacity>Consciousness</capacity>
					  <offset>0.2</offset>
					</li>
				</capMods>
				<statOffsets>
					<WorkSpeedGlobal>0.2</WorkSpeedGlobal>
					<MentalBreakThreshold>0.08</MentalBreakThreshold>
				</statOffsets>
			</li>
		</stages>
  </HediffDef>
<!--Age-->

  <HediffDef Name="AndroidDiseaseBase" Abstract="True">
    <hediffClass>HediffWithComps</hediffClass>
    <defaultLabelColor>(0.8, 0.8, 0.35)</defaultLabelColor>
    <initialSeverity>0.001</initialSeverity>
    <comps>
      <li Class="HediffCompProperties_Discoverable">
        <sendLetterWhenDiscovered>true</sendLetterWhenDiscovered>
      </li>
    </comps>
  </HediffDef>
  
  <HediffDef ParentName="AndroidDiseaseBase">

    <defName>DecayedFrame</defName>
    <label>decaying mainframe</label>
    <labelNoun>a decaying mainframe</labelNoun>
		<defaultLabelColor>(0.8, 0.8, 0.5)</defaultLabelColor>
    <stages>
      <li>
        <capMods>
          <li>
            <capacity>Moving</capacity>
            <offset>-0.30</offset>
          </li>
          <li>
            <capacity>Manipulation</capacity>
            <offset>-0.10</offset>
          </li>
        </capMods>
      </li>
    </stages>
  </HediffDef>
  
  <HediffDef ParentName="AndroidDiseaseBase">

    <defName>CorruptMemory</defName>
    <label>corrupt memory</label>
		<defaultLabelColor>(0.8, 0.8, 0.5)</defaultLabelColor>
    <stages>
      <li>
				<partEfficiencyOffset>-0.35</partEfficiencyOffset>
        <capMods>
          <li>
            <capacity>Talking</capacity>
            <offset>-0.25</offset>
          </li>
          <li>
            <capacity>Hearing</capacity>
            <offset>-0.25</offset>
          </li>
        </capMods>
        <mentalStateGivers>
          <li>
            <mentalState>WanderConfused</mentalState>
            <mtbDays>5</mtbDays>
          </li>
        </mentalStateGivers>
      </li>
    </stages>
  </HediffDef>
  
  <HediffDef ParentName="AndroidDiseaseBase">

    <defName>FaultyPump</defName>
    <label>faulty pump</label>
    <labelNoun>a faulty pump</labelNoun>
    <lethalSeverity>1</lethalSeverity>
		<defaultLabelColor>(0.8, 0.8, 0.5)</defaultLabelColor>
    <comps>

      <li Class="HediffCompProperties_Immunizable">
        <severityPerDayNotImmune>0.0007</severityPerDayNotImmune>
        <severityPerDayNotImmuneRandomFactor>
          <min>0.5</min>
          <max>3</max>
        </severityPerDayNotImmuneRandomFactor>
      </li>
    </comps>
    <stages>
      <li>
        <label>minor</label>
				<partEfficiencyOffset>-0.05</partEfficiencyOffset>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>BCoolantPump</hediff>
            <mtbDays>600</mtbDays>
            <partsToAffect>
              <li>MHeart</li>
            </partsToAffect>
          </li>
        </hediffGivers>
      </li>
      <li>
        <minSeverity>0.2</minSeverity>
        <label>minor</label>
				<partEfficiencyOffset>-0.1</partEfficiencyOffset>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>BCoolantPump</hediff>
            <mtbDays>500</mtbDays>
            <partsToAffect>
              <li>Heart</li>
            </partsToAffect>
          </li>
        </hediffGivers>
      </li>
      <li>
        <minSeverity>0.4</minSeverity>
        <label>major</label>
				<partEfficiencyOffset>-0.15</partEfficiencyOffset>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>BCoolantPump</hediff>
            <mtbDays>300</mtbDays>
            <partsToAffect>
              <li>MHeart</li>
            </partsToAffect>
          </li>
        </hediffGivers>
      </li>
      <li>
        <minSeverity>0.6</minSeverity>
        <label>major</label>
				<partEfficiencyOffset>-0.35</partEfficiencyOffset>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>BCoolantPump</hediff>
            <mtbDays>200</mtbDays>
            <partsToAffect>
              <li>MHeart</li>
            </partsToAffect>
          </li>
        </hediffGivers>
      </li>
      <li>
        <minSeverity>0.9</minSeverity>
        <label>extreme</label>
        <lifeThreatening>true</lifeThreatening>
				<partEfficiencyOffset>-0.6</partEfficiencyOffset>
        <hediffGivers>
          <li Class="HediffGiver_Random">
            <hediff>BCoolantPump</hediff>
            <mtbDays>120</mtbDays>
            <partsToAffect>
              <li>MHeart</li>
            </partsToAffect>
          </li>
        </hediffGivers>
      </li>
    </stages>
  </HediffDef>
<!--Disease or illness-->

  <HediffDef>
    <defName>BatteryChargeMech</defName>
    <label>Battery</label>
	    <hediffClass>HediffWithComps</hediffClass>
    <initialSeverity>1</initialSeverity>
		  		<defaultLabelColor>(0.3, 0.7, 0.3)</defaultLabelColor>
    <scenarioCanAdd>false</scenarioCanAdd>
	    <comps>
      <li Class="HediffCompProperties_SeverityPerDay">
        <severityPerDay>-0.5</severityPerDay>
      </li>
    </comps>
    <stages>
      <li>
	  <minSeverity>0</minSeverity>
        <label>Empty</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <setMax>0</setMax>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.1</minSeverity>
        <label>Critical</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.3</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.25</minSeverity>
        <label>Low</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.2</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.45</minSeverity>
        <label>Moderate</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.1</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.6</minSeverity>
        <label>High</label>
        <capMods>
          <li>
            <capacity>Consciousness</capacity>
            <offset>-0.05</offset>
          </li>
        </capMods>
      </li>
      <li>
        <minSeverity>0.8</minSeverity>
        <label>Full</label>
      </li>
    </stages>
  </HediffDef>
	
	
	<HediffDef>
		<defName>AndroidCoolantLoss</defName>
		<label>coolant loss</label>
		<maxSeverity>1.0</maxSeverity>
	<defaultLabelColor>(0.6, 0.6, 0.9)</defaultLabelColor>
      <stages>
        <li>
          <label>Hidden.</label>
          <becomeVisible>false</becomeVisible>
        </li>
		
		
        <li>
          <minSeverity>0.1</minSeverity>
          <label>Minor</label>
		  <statOffsets>
           <ComfyTemperatureMax>-5</ComfyTemperatureMax>
		  </statOffsets>
        </li>
		
		
        <li>
          <minSeverity>0.3</minSeverity>
          <label>Major</label>
		  <statOffsets>
           <ComfyTemperatureMax>-15</ComfyTemperatureMax>
		  </statOffsets>
            <capMods>
              <li>
                <capacity>Moving</capacity>
                <offset>-0.05</offset>
              </li>
			   <li>
                <capacity>Manipulation</capacity>
                <offset>-0.05</offset>
              </li>
            </capMods>
        </li>
		
		
        <li>
          <minSeverity>0.6</minSeverity>
          <label>Severe</label>
		  <statOffsets>
		<ComfyTemperatureMax>-20</ComfyTemperatureMax>
		  </statOffsets>
            <capMods>
              <li>
                <capacity>Consciousness</capacity>
                <offset>-0.2</offset>
              </li>
              <li>
                <capacity>Moving</capacity>
                <offset>-0.1</offset>
              </li>
			   <li>
                <capacity>Manipulation</capacity>
                <offset>-0.1</offset>
              </li>
            </capMods>
        </li>
		
		
        <li>
          <minSeverity>0.9</minSeverity>
          <label>Extreme</label>
		  <statOffsets>
         <ComfyTemperatureMax>-40</ComfyTemperatureMax>
		  </statOffsets>
            <capMods>
              <li>
                <capacity>Consciousness</capacity>
                <offset>-0.4</offset>
              </li>
              <li>
                <capacity>Moving</capacity>
                <offset>-0.30</offset>
              </li>
			   <li>
                <capacity>Manipulation</capacity>
                <offset>-0.30</offset>
              </li>
            </capMods>
        </li>
	</stages>
	</HediffDef>

